pipeline {
    agent any

   tools {
     jdk 'Java8'
     maven "Maven3.3.9"
   }
 
    stages {
        stage('git clone') {
          steps {
              git 'https://github.com/VG19434/game2.git'
          }
        }
        stage('Compile and Build') {
            steps {
                sh 'mvn clean install -u -Dmaven.test.skip=true'
            }
        }
    }
}
